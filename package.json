{
  "author": "ns8inc",
  "dependencies": {
    "@ns8/ns8-switchboard-operator": "1.0.130",
    "@types/aws-lambda": "^8.10.49",
    "@types/aws4": "^1.5.1",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.662.0",
    "aws4": "^1.9.1",
    "axios": "^0.19.2",
    "country-list": "2.2.0",
    "is-ip": "3.1.0",
    "moment": "2.24.0",
    "ns8-protect-models": "1.0.273",
    "ns8-switchboard-interfaces": "1.0.121",
    "prompts": "2.3.2",
    "serverless-pseudo-parameters": "2.5.0",
    "serverless-step-functions": "2.18.0",
    "typescript": "3.8.3",
    "ua-parser-js": "0.7.21",
    "uuid": "7.0.3"
  },
  "description": "Protect Switchboard SDK",
  "devDependencies": {
    "@ns8/protect-tools-js": "0.1.4",
    "@types/country-list": "^2.1.0",
    "@types/sinon": "^9.0.0",
    "@types/ua-parser-js": "0.7.33",
    "@types/uuid": "7.0.2",
    "sinon": "^9.0.2"
  },
  "envDocumentation": {
    "AWS_SERVERLESS_YML": {
      "defaultValue": "node_modules/@ns8/protect-sdk-switchboard/serverless.common.yml",
      "description": "The location of the serverless config. The default value does not need to be changed."
    },
    "AWS_SERVICE_NAME": {
      "defaultValue": "",
      "description": "(Optional) name of the switchboard project."
    },
    "DEV_SUFFIX": {
      "defaultValue": "dev",
      "description": "Developer initials to use in the construction of the lambda and step function ARNs."
    },
    "NODE_ENV": {
      "defaultValue": "dev",
      "description": "Environment. Options are: dev, test and prod."
    },
    "SYNC_PEER_DEPENDENCIES": {
      "defaultValue": true,
      "description": "If true, sets all project dependencies as peer dependencies"
    }
  },
  "files": [
    "dist",
    "serverless.common.yml"
  ],
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "name": "@ns8/protect-sdk-switchboard",
  "peerDependencies": {
    "@ns8/ns8-switchboard-operator": "1.0.130",
    "@types/aws-lambda": "^8.10.49",
    "@types/aws4": "^1.5.1",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.662.0",
    "aws4": "^1.9.1",
    "axios": "^0.19.2",
    "country-list": "2.2.0",
    "is-ip": "3.1.0",
    "moment": "2.24.0",
    "ns8-protect-models": "1.0.273",
    "ns8-switchboard-interfaces": "1.0.121",
    "prompts": "2.3.2",
    "serverless-pseudo-parameters": "2.5.0",
    "serverless-step-functions": "2.18.0",
    "typescript": "3.8.3",
    "ua-parser-js": "0.7.21",
    "uuid": "7.0.3"
  },
  "repository": "https://github.com/ns8inc/protect-sdk-switchboard.git",
  "scripts": {
    "build": "yarn clean && yarn install && yarn generate:exports && yarn build:scripts && yarn build:project && yarn docs:all && yarn sortJson",
    "build:dev": "yarn && NODE_ENV=dev yarn build && yarn link",
    "build:prod": "yarn && NODE_ENV=prod yarn build",
    "build:project": "tsc -d && yarn bundle",
    "build:scripts": "tsc -p tsconfig.build.json --outDir dist/build ",
    "bundle": "webpack",
    "clean": "rm -rf .awcache .tmp dist .nyc_output coverage api temp",
    "count": "cloc ./src ./build ./docs",
    "deploy": "METHOD=deploy ts-node -P ./build/tsconfig.json ./build/lambdaDeploy.ts",
    "docs:all": "yarn docs:standardize && yarn docs:api",
    "docs:api": "npx ./node_modules/@ns8/protect-tools-js/dist/docs/generateApi.js --path=./api-extractor.json",
    "docs:publish": "npx ./node_modules/@ns8/protect-tools-js/dist/docs/generateApiDocs.js",
    "docs:standardize": "npx ./node_modules/@ns8/protect-tools-js/dist/readme/standardizeReadme.js",
    "generate:exports": "yarn cti create ./src -w",
    "lint": "yarn eslint -c 'node_modules/@ns8/protect-tools-js/.eslintrc.yml' 'src/**/*.ts' 'docs/**/*.md'",
    "lint:fix": "yarn eslint -c 'node_modules/@ns8/protect-tools-js/.eslintrc.yml' --fix 'src/**/*.ts' 'docs/**/*.md'",
    "sortJson": "npx ./node_modules/@ns8/protect-tools-js/dist/lint/sortJson.js",
    "test": "nyc --nycrc-path 'node_modules/@ns8/protect-tools-js/nycrc.yml' mocha 'src/*/*.test.ts' && yarn count",
    "test:coverage": "nyc yarn test:debug && yarn count",
    "test:debug": "mocha -r ts-node/register 'src/*/*.test.ts'",
    "undeploy": "METHOD=undeploy ts-node -P ./build/tsconfig.json ./build/lambdaDeploy.ts"
  },
  "scriptsDocumentation": {
    "build": {
      "description": "Assembles `src` code into a single, minified JS module with type definitions. Exports `build` scripts into a build folder.",
      "dev": false
    },
    "build:dev": {
      "description": "Builds in dev mode.",
      "dev": true
    },
    "build:prod": {
      "description": "Builds in production mode.",
      "dev": true
    },
    "build:project": {
      "description": "Compiles and bundles the `src` code.",
      "dev": true
    },
    "build:scripts": {
      "description": "Compiles the build scripts.",
      "dev": true
    },
    "bundle": {
      "description": "Runs WebPack on the `src` code.",
      "dev": false
    },
    "clean": {
      "description": "Purges all temporary folders",
      "dev": true
    },
    "count": {
      "description": "Counts lines of source code.",
      "dev": true
    },
    "deploy": {
      "description": "Deploys the polling lamdbas to AWS.",
      "dev": true
    },
    "docs:all": {
      "description": "Standardizes markdown and generates the API metadata.",
      "dev": true
    },
    "docs:api": {
      "description": "Creates a `project-api` Markdown in docs and an `index.d.ts` file in dist.",
      "dev": true
    },
    "docs:publish": {
      "description": "Generates end-to-end documentation for the entire project and publishes it to the `gh-pages` branch.",
      "dev": true
    },
    "docs:standardize": {
      "description": "Creates or updates a new readme with a standard set of readme sections, including a toc, yarn script documention, links to repo documentation files and an NS8 license",
      "dev": true
    },
    "generate:exports": {
      "description": "Generates index.ts files for all exports recursively in the 'src' folder",
      "dev": true
    },
    "lint": {
      "description": "Lints the codebase and the documentation",
      "dev": true
    },
    "lint:fix": {
      "description": "Lints the codebase and automatically fixes what it can",
      "dev": true
    },
    "sortJson": {
      "description": "Performs aesthetic operations to make the project files easier to navigate and read",
      "dev": true
    },
    "test": {
      "description": "Runs tests and calculates test coverage",
      "dev": true
    },
    "test:coverage": {
      "description": "Calculates test coverage",
      "dev": true
    },
    "test:debug": {
      "description": "Runs tests with the debugger",
      "dev": true
    },
    "undeploy": {
      "description": "Removes the polling lamdbas from AWS",
      "dev": true
    }
  },
  "types": "dist/index.d.ts",
  "version": "0.2.31"
}
